<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IS4439_CA2</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/vendor/fontawesome/css/all.min.css" />
    <link rel="stylesheet" href="~/vendor/assets/css/templatemo-style.css" />
    <link rel="stylesheet" href="~/css/layout.css" />

</head>
<body>
    <div id="loader-wrapper">
        <div id="loader"></div>

        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <div style="width:100%">

        <header>
            <nav class="navbar navbar-expand-lg">
                <div class="container-fluid">

                    <a class="navbar-brand" asp-controller="Projects" asp-action="Index">
                        <i class="fas fa-camera"></i>
                        Shay Mckenna Design
                    </a>
                    <div class="wrapper divLine">

                        <div id="navbarSupportedContent">
                            <ul class="navbar-nav ml-auto mb-2 mb-lg-0">
                                <li class="nav-item">
                                    <a class="nav-link nav-link-1" asp-controller="Projects" asp-action="Index">Photos</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link nav-link-2" asp-route="VideoProjects">Videos</a>
                                </li>
                                <partial name="_LoginPartial" />
                            </ul>
                        </div>
                
                    </div>
                </div>
            </nav>
        </header>
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <footer class="tm-bg-gray pt-5 pb-3 tm-text-gray tm-footer">
            <div class="container-fluid tm-container-small">
                <div class="row">
                    <div class="col-md-7">
                        <ul class="tm-social-links d-flex justify-content-end pl-0 mb-5">
                            <li class="mb-2"><a href="shaycreativedesign@gmail.com"><i class="fab fa-google" aria-hidden="true"></i></a></li>
                            <li class="mb-2"><a href="https://www.instagram.com/sdesignssss/"><i class="fab fa-instagram"></i></a></li>
                        </ul>
                        <a class="tm-text-gray text-right d-block mb-2">Terms of Use</a>
                        <a class="tm-text-gray text-right d-block">Privacy Policy</a>
                    </div>
                </div>
            </div>
        </footer>
    

    </div>
    <script src="~/vendor/assets/js/plugins.js"></script>

    <script>
        $(window).on("load", function () {
            $('body').addClass('loaded');
        });</script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
